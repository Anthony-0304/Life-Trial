<%= simple_form_for(@listing) do |f| %>
  <%= f.input :title %>
  <%= f.input :category, as: :select, collection: [
    'Actors & Actresses',
    'Musicians',
    'Record Breakers',
    'Royalty',
    'Sports People'
    ], prompt: 'Select Category' %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit class: "btn btn-primary"%>
<% end %>
