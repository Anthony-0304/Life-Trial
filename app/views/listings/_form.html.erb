<%= simple_form_for(@listing) do |f| %>
  <%= f.input :title %>
  <%= f.input :category, as: :select, collection: [
    'Comedy',
    'Commoner',
    'Film/TV',
    'Influencer',
    'Music',
    'Politics',
    'Royalty',
    'Sport'
    ], prompt: 'Select Category' %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit class: "btn btn-primary"%>
<% end %>
